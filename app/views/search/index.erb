<h1><%= @stores.count %> Total Stores</h1>
<% @stores.each do |store| %>
<ul class="store">
  <li class="name">Name: <%= store.longname %></li>
  <li class="city">City: <%= store.city %></li>
  <li class="distance">Distance: <%= store.distance %></li>
  <li class="phone">Phone Number: <%= store.phone %></li>
  <li class="type">Store Type: <%= store.storetype %></li>
</ul>
<% end %>
